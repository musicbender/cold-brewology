{
  "name": "cold-brewology",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "start": "export NODE_ENV=development; webpack-dev-server --env=dev --hot --progress --host 0.0.0.0 --config webpack.dev.config.js",
    "bs": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.server.config.js",
    "bc": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.prod.config.js",
    "build": "npm run clean && npm run bc && npm run bs",
    "open:dist": "export NODE_ENV=production; export LIVE=false; export PORT=3001; nodemon index.js",
    "build:open": "npm run build && npm run open:dist"
  },
  "keywords": [],
  "author": "Pat Jacobs",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "pug": "^2.0.0-beta3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "mochawesome": "^2.3.1",
    "node-sass": "^4.7.2",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "rsync": "^0.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
